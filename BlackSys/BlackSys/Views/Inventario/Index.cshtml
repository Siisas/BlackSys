@model IEnumerable<BlackSys.Database.Inventario>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CantidadEntrada)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CantidadSalidad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaIngreso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Producto.Descripcion_porducto)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CantidadEntrada)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CantidadSalidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaIngreso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Producto.Descripcion_porducto)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id_Inventartio }) |
            @Html.ActionLink("Details", "Details", new { id=item.id_Inventartio }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id_Inventartio })
        </td>
    </tr>
}

</table>
